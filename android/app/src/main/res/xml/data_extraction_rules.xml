<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup disableIfUnsupported="true">
        <exclude domain="sharedprefs" path="."/>
        <exclude domain="database" path="."/>
        <exclude domain="file" path="/unencrypted_files/"/>
        <exclude domain="file" path="/encrypted_files/"/>
    </cloud-backup>
    <device-transfer>
        <exclude domain="sharedprefs" path="."/>
        <exclude domain="database" path="."/>
        <exclude domain="file" path="/unencrypted_files/"/>
        <exclude domain="file" path="/encrypted_files/"/>
    </device-transfer>
</data-extraction-rules>